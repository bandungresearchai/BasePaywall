# Environment variables for Next.js frontend

# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY="your_onchainkit_api_key"

# Deployed contract address
NEXT_PUBLIC_CONTRACT_ADDRESS="0xYourDeployedContractAddress"

# Base RPC URLs
NEXT_PUBLIC_BASE_RPC_URL="https://mainnet.base.org"
NEXT_PUBLIC_BASE_SEPOLIA_RPC_URL="https://sepolia.base.org"

# Network environment: "sepolia" or "mainnet"
NEXT_PUBLIC_NETWORK="sepolia"

# WalletConnect Project ID (optional, for WalletConnect support)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID="your_walletconnect_project_id"

# Privy App ID (optional, for social login support - email, Google, Twitter)
# Get from https://console.privy.io/
NEXT_PUBLIC_PRIVY_APP_ID="your_privy_app_id"
